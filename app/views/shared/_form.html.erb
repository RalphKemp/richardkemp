<%= simple_form_for(@album) do |f| %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <div class="attachinary-photos-list">
      <%= f.submit class: "submit-album"%>
      <%= f.attachinary_file_field :photos, as: :attachinary %>
    </div>
  </div>
<% end %>

