<%= simple_form_for(@post) do |f| %>
  <%= f.collection_select(:category_id, Category.all, :id, :name, {prompt: “Choose a category” }) %> <br>
  <%= f.input :title %>
  <%= f.input :content %>
  <%= f.attachinary_file_field :photos, as: :attachinary, cloudinary: {use_filename: true, unique_filename: false}%>
  <%= f.submit class: "submit-post"%>
<% end %>







