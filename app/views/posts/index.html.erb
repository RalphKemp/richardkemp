<div class="container">
  <div class="row" id="new_posts">
    <% @posts.each do |post| %>
      <%= render post %>
    <% end %>
  </div>
</div>


<% if user_signed_in? %>
  <div class="new-button">
    <%= link_to "new blog post", new_post_path, id: "new_post_link", remote: true %>
  </div>
<% end %>




